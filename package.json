{
  "name": "@lennygir/leg-scaffolding",
  "version": "1.1.0",
  "description": "Scaffolding tool for [rest-api-node-template](https://github.com/lennygir/rest-api-node-template)",
  "keywords": [
    "rest",
    "api",
    "scaffolding",
    "controllers",
    "middlewares",
    "repositories",
    "interceptors"
  ],
  "main": "index.mjs",
  "scripts": {
    "test": "eslint \"./**/*.mjs\"",
    "publish-npm": "npm run test && npm publish --access=public",
    "publish-git": "npm run test && git add * && git commit -m",
    "postinstall": "git config core.hooksPath ./.git/hooks"
  },
  "bin": {
    "leg": "bin/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lennygir/leg-scaffolding.git"
  },
  "author": "Lenny GIRARDOT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lennygir/leg-scaffolding/issues"
  },
  "homepage": "https://github.com/lennygir/leg-scaffolding#readme",
  "devDependencies": {
    "eslint": "^8.9.0"
  }
}
